#############################################################################
#说明：																																			#
#release版本统一使用udi2目录下头文件，																			#
#debug和stepbystep版本时只有中间件各模块使用udi2_proxy目录头文件,						#
#porting编译时不能包含udi2_proxy目录																				#
#############################################################################

PORTING_DIR := $(shell pwd | grep "/porting/")

ifeq "$(PORTING_DIR)" ""
MMCP_UDI2_PROXY := y
endif

ifeq "$(MMCP_UDI2)" "y"
ifeq "$(MMCP_COMPILING_MODE)" "release"
INCLUDES += -I$(MMCP_HOME)/include/porting/udi2
else
ifeq "$(MMCP_UDI2_PROXY)" "y"
#INCLUDES += -I$(MMCP_HOME)/include/porting/udi2_proxy
INCLUDES += -I$(MMCP_HOME)/include/porting/udi2
else
INCLUDES += -I$(MMCP_HOME)/include/porting/udi2
endif
endif
else
INCLUDES += -I$(MMCP_HOME)/include/porting/udi2
endif

ifneq "$(MMCP_UDI2)" "y"
INCLUDES += \
	-I$(MMCP_HOME)/include/porting/platform \
	-I$(MMCP_HOME)/include/porting/hdi \
	-I$(MMCP_HOME)/include/porting/udi \
	-I$(MMCP_HOME)/include/porting/csplus \
	-I$(MMCP_HOME)/include/porting/deprecated
endif

ifeq "$(MMCP_QT)" "y"
INCLUDES += -I$(MMCP_HOME)/include/webkit
endif
