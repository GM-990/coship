# Kconfig options for JSExt

###################################################	  	
config MMCP_JSEXT_SUPPORT_DVB
	bool "Jsext support dvb"
	default y
	help
		if jsext support dvb,please
		select yes,else select no!
###################################################	 	

###################################################	  	
config SUPPORT_PROJECT_TJBANK
	bool "SUPPORT_PROJECT_TJBANK"
	default n
	help
		if jsext support TJBank,please
		select yes,else select no!
###################################################

